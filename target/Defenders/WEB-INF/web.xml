<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1"
         metadata-complete="true">
    <display-name>Archetype Created Web Application</display-name>
    <filter>
        <filter-name>Set Character Encoding</filter-name>
        <filter-class>org.apache.catalina.filters.SetCharacterEncodingFilter</filter-class>
        <init-param>
            <param-name>encoding</param-name>
            <param-value>UTF-8</param-value>
        </init-param>
        <init-param>
            <param-name>ignore</param-name>
            <param-value>true</param-value>
        </init-param>
    </filter>

    <filter-mapping>
        <filter-name>Set Character Encoding</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>

    <servlet>
        <servlet-name>indexServlet</servlet-name>
        <servlet-class>servlets.indexServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>indexServlet</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>dareDevilServlet</servlet-name>
        <servlet-class>servlets.dareDevilServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dareDevilServlet</servlet-name>
        <url-pattern>/dareDevil</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>jessicaJonesServlet</servlet-name>
        <servlet-class>servlets.jessicaJonesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>jessicaJonesServlet</servlet-name>
        <url-pattern>/jessicaJones</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ironFistServlet</servlet-name>
        <servlet-class>servlets.ironFistServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ironFistServlet</servlet-name>
        <url-pattern>/ironFist</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>lukeCageServlet</servlet-name>
        <servlet-class>servlets.lukeCageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>lukeCageServlet</servlet-name>
        <url-pattern>/lukeCage</url-pattern>
    </servlet-mapping>




</web-app>
